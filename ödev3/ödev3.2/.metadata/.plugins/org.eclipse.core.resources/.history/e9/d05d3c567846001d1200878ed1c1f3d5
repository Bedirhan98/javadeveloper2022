package package1;

import classs.CreditManager;

public class CustomerManager {
	private Customer customer;
	public CustomerManager(Customer customer) {
		this.customer=customer;
	}
	
	public void add(){
		System.out.println("Müşteri eklendi : "+customer.firsName+" "+customer.lastName);
	}
	public void delete(){
		System.out.println("Müşteri silindi : "+customer.firsName+" "+customer.lastName);
	}

}
